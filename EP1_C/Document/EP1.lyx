\begin_layout Plain Layout

#include <stdio.h>
\end_layout

\begin_layout Plain Layout

#include <math.h>
\end_layout

\begin_layout Plain Layout

#include <stdlib.h>
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

/*
\end_layout

\begin_layout Plain Layout

 * Global variables
\end_layout

\begin_layout Plain Layout

*/
\end_layout

\begin_layout Plain Layout

double M = 2.0;
\end_layout

\begin_layout Plain Layout

double gama = 0.0001;
\end_layout

\begin_layout Plain Layout

double epsilon = 0.0000000001; /* 10^(-10) its practicaly 0*/
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

/*
\end_layout

\begin_layout Plain Layout

 * Objective function: Rosenbrock function
\end_layout

\begin_layout Plain Layout

 * f(x1,x2) = 100(x2-x1^2)^2 + (1-x1)^2
\end_layout

\begin_layout Plain Layout

 * x* = (1,1)^T is the only local minimizator
\end_layout

\begin_layout Plain Layout

*/
\end_layout

\begin_layout Plain Layout

void Rosenbrock(double* xk, double* output)
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

    *output = 100*(xk[1]-xk[0]*xk[0])*(xk[1]-xk[0]*xk[0]) + (1-xk[0])*(1-xk[0]);
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

/*
\end_layout

\begin_layout Plain Layout

 * First derivate of the Rosenbrock function in R2
\end_layout

\begin_layout Plain Layout

 * f'(x1,x2) = [400*x1^3 -400*x2*x1 +2*x1 - 2 ; 200(x2 - x1^2) ]
\end_layout

\begin_layout Plain Layout

*/
\end_layout

\begin_layout Plain Layout

void RosenbrockD(double* xk, double* output)
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

    output[0] = 400*xk[0]*xk[0]*xk[0] - 400*xk[1]*xk[0] + 2*xk[0] -2;
\end_layout

\begin_layout Plain Layout

    output[1] = 200*(xk[1]-xk[0]*xk[0]);
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

/*
\end_layout

\begin_layout Plain Layout

 * Function that return the minimum valor with linear search
\end_layout

\begin_layout Plain Layout

*/
\end_layout

\begin_layout Plain Layout

void returnMin(double* xk)
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

    double* d     = (double*)malloc(2*sizeof(double));
\end_layout

\begin_layout Plain Layout

    double* fD    = (double*)malloc(2*sizeof(double));
\end_layout

\begin_layout Plain Layout

    double* xkNew = (double*)malloc(2*sizeof(double));
\end_layout

\begin_layout Plain Layout

    double f, fNew, alfa;
\end_layout

\begin_layout Plain Layout

    int iterations = 0;
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

    while(iterations < 1000000000) /* Stop by iterations */
\end_layout

\begin_layout Plain Layout

    {
\end_layout

\begin_layout Plain Layout

        /* Basic parameters */
\end_layout

\begin_layout Plain Layout

        alfa = 1;
\end_layout

\begin_layout Plain Layout

        Rosenbrock(xk, &f);
\end_layout

\begin_layout Plain Layout

        RosenbrockD(xk, fD);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        /* Calculate the gradient and multiply by -1 */
\end_layout

\begin_layout Plain Layout

        d[0]=(-1)*fD[0];
\end_layout

\begin_layout Plain Layout

        d[1]=(-1)*fD[1];
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        /* Check stop conditions: The two norm */
\end_layout

\begin_layout Plain Layout

        if( sqrt(d[0]*d[0] + d[1]*d[1]) < epsilon )
\end_layout

\begin_layout Plain Layout

            break;
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        /* Calculate a possible new xk */
\end_layout

\begin_layout Plain Layout

        xkNew[0]= xk[0]+alfa*d[0];
\end_layout

\begin_layout Plain Layout

        xkNew[1]= xk[1]+alfa*d[1];
\end_layout

\begin_layout Plain Layout

        Rosenbrock(xkNew, &fNew);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        /* Calculate a new alfa */
\end_layout

\begin_layout Plain Layout

        while( fNew > ( f + gama*alfa*(fD[0]*d[0] + fD[1]*d[1])  ))
\end_layout

\begin_layout Plain Layout

        {
\end_layout

\begin_layout Plain Layout

			alfa = alfa / M;
\end_layout

\begin_layout Plain Layout

            xkNew[0]= xk[0]+alfa*d[0];
\end_layout

\begin_layout Plain Layout

            xkNew[1]= xk[1]+alfa*d[1];
\end_layout

\begin_layout Plain Layout

            Rosenbrock(xkNew, &fNew);
\end_layout

\begin_layout Plain Layout

        }
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        /* Update xk */
\end_layout

\begin_layout Plain Layout

        xk[0]=xkNew[0];
\end_layout

\begin_layout Plain Layout

        xk[1]=xkNew[1];
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        iterations++;
\end_layout

\begin_layout Plain Layout

    }
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

    /* Print the result */
\end_layout

\begin_layout Plain Layout

    printf("Point (%f,%f) Iterations:%d
\backslash
n",xk[0],xk[1],iterations);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

    /* Memory free */
\end_layout

\begin_layout Plain Layout

    free(fD);
\end_layout

\begin_layout Plain Layout

    free(d);
\end_layout

\begin_layout Plain Layout

    free(xkNew);
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

int main(void)
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

    double i,j;
\end_layout

\begin_layout Plain Layout

    double limit = 50;
\end_layout

\begin_layout Plain Layout

    double step = 0.5;
\end_layout

\begin_layout Plain Layout

    double* x0 = (double*)malloc(2*sizeof(double));
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

    for (i = -limit; i <= limit; i+=step)
\end_layout

\begin_layout Plain Layout

    {
\end_layout

\begin_layout Plain Layout

        for (j = -limit; j <= limit; j+=step)
\end_layout

\begin_layout Plain Layout

        {
\end_layout

\begin_layout Plain Layout

            x0[0]=i;
\end_layout

\begin_layout Plain Layout

            x0[1]=j;
\end_layout

\begin_layout Plain Layout

            printf("x0=(%f,%f) ",x0[0],x0[1]);
\end_layout

\begin_layout Plain Layout

            returnMin(x0);
\end_layout

\begin_layout Plain Layout

        }
\end_layout

\begin_layout Plain Layout

    }
\end_layout

\begin_layout Plain Layout

    return 0;
\end_layout

\begin_layout Plain Layout

}
